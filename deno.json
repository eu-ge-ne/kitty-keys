{
  "name": "@eu-ge-ne/kitty-keys",
  "version": "0.9.0",
  "license": "MIT",
  "exports": "./src/mod.ts",
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.14"
  },
  "publish": {
    "exclude": [
      ".github",
      ".vscode",
      "bin",
      "test",
      "codecov.yml"
    ]
  },
  "compilerOptions": {
    "noUncheckedIndexedAccess": true
  },
  "lint": {
    "rules": {
      "tags": ["recommended", "jsr"],
      "include": [
        "eqeqeq",
        "explicit-module-boundary-types",
        "explicit-function-return-type",
        "no-console",
        "no-eval",
        "no-external-import"
      ],
      "exclude": ["no-control-regex"]
    }
  }
}
